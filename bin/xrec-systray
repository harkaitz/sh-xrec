#!/bin/sh -e
##:
#h: Usage: xrec-systray -V
#h:
#h: Control xrec(1) from the systray using sicon(1).
##:
. xrec
xrec_systray() {
    
    ## Create FIFO.
    rm -f "${XREC_FIFO}"
    mkfifo "${XREC_FIFO}"
    
    sicon title="XREC"                     \
          tooltip="Screen recorder"        \
          icon=b64:"${ICON_GREEN}"         \
          < "${XREC_FIFO}" | xrec_systray_loop > "${XREC_FIFO}"
}
xrec_systray_loop() {
    local job cmd pid='' jobs="$(xrec -l)"
    
    ## Create buttons.
    for job in ${jobs}; do
        echo menu_add="R${job}|${job}: Start recording."
        echo menu_add="V${job}|${job}: View recording."
        echo menu_add="C${job}|${job}: Clear recordings."
    done
    echo menu_add_separator=y
    echo menu_add='stop|Stop recording'
    echo menu_add_quit='Close'
    echo menu_hide='stop'
    
    ## Enter loop.
    while read -r cmd; do
        if test -n "${pid}"; then
            pkill -INT ffmpeg || true
            wait
            pid=
        fi
        case "${cmd}" in
            stop)  
                for job in ${jobs}; do
                    echo menu_show="R${job}"
                    echo menu_show="V${job}"
                    echo menu_show="C${job}"
                done
                echo menu_hide="stop"
                echo icon=b64:"${ICON_GREEN}"
                ;;
            R*)
                xrec -r "${cmd#R}" >&2 &
                pid="$!"
                for job in ${jobs}; do
                    echo menu_hide="R${job}"
                    echo menu_hide="V${job}"
                    echo menu_hide="C${job}"
                done
                echo menu_show="stop"
                echo icon=b64:"${ICON_RED}"
                ;;
            V*)
                xrec -p "${cmd#V}" >&2
                ;;
            C*)
                xrec -c "${cmd#C}" >&2
                ;;
        esac
    done
    
    ## Cleanup.
    if test -n "${pid}"; then
        pkill -INT ffmpeg || true
        wait
        pid=
    fi
}
xrec_systray_show_variables() {
    echo "XREC_DIRECTORY : ${XREC_DIRECTORY}"
    echo "XREC_FIFO      : ${XREC_FIFO}"
    echo "XREC_PID       : ${XREC_PID}"
}
xrec_systray_calc_variables() {
    XREC_FIFO="${XREC_FIFO:-${HOME}/.xrec.fifo}"
    XREC_PID="${XREC_PID:-${HOME}/.xrec.pid}"
}
## -------------------------------------------------------------------
xrec_systray_data() {
    ICON_GREEN=''
    ICON_RED=''
}
xrec_systray_calc_variables
xrec_systray_data
if test @"${SCRNAME:-$(basename "$0")}" = @"xrec-systray"; then
    case "${1}" in
        -h|--help) sed -n 's/^ *#h: \{0,1\}//p' "$0";;
        -*V*)      xrec_systray_show_variables;;
        *)         xrec_systray "$@"; exit 0;;
    esac
fi
